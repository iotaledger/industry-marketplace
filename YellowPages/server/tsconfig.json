{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      // "dom",
      "esnext",
      // "es2017.iterable"
    ],
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "typeRoots": [
      "node_modules/@types",
      "types"
    ],
    "outDir": "build",
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ]
}
