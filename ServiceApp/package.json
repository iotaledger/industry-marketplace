    "@iota/converter": "1.0.0-beta.30",
    "@iota/mam.js": "github:iotaledger/mam.js",
    "axios": "^0.21.1",
    "date-fns": "^1.30.1",
    "highlight.js": "^9.18.1",
    "industry_4.0_language": "iotaledger/industry_4.0_language",
    "lodash-es": "^4.17.15",
    "query-string": "^6.13.4",
    "react": "16.13.1",
    "react-clipboard.js": "^2.0.16",
    "react-cookie": "^4.0.3",
    "react-datepicker": "^2.14.1",
    "react-dom": "16.13.1",
    "react-is": "^16.13.1",
    "react-md": "^1.16.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "socket.io-client": "^2.3.1",
    "styled-components": "5.2.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "install-all": "yarn && cd server && yarn",
    "client": "yarn start",
    "server": "cd server && yarn start-dev",
    "server-build": "cd server && yarn build",
    "dev": "yarn install-all && yarn server-build && concurrently --kill-others-on-fail \"yarn client\" \"cd server && yarn start-dev\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "new-wallet": "cd server/build/src/utils/ && node userHelper.js --create wallet",
    "new-user": "f(){ cd server/build/src/utils/ && node userHelper.js --create user --role $1 --name $2 --location $3;};f"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=10.0"
  },
  "proxy": "http://localhost:4000/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "resolutions": {
    "acorn": "^7.1.1",
    "handlebars": "^4.5.3",
    "http-proxy": "^1.18.1",
    "kind-of": "^6.0.3",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "node-fetch": "^2.6.1",
    "node-forge": "^0.10.0",
    "websocket-extensions": "^0.1.4",
    "yargs-parser": "^13.1.2"
  }
}
