{
  "name": "SeMarket",
  "version": "0.1.0",
  "description": "SeMarket",
  "repository": {
    "type": "git",
    "url": "https://github.com/iotaledger/SeMarket.git"
  },
  "private": true,
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "lodash-es": "^4.17.11",
    "react": "16.8.6",
    "react-datepicker": "^2.7.0",
    "react-dom": "16.8.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "SeMarket": "iotaledger/SeMarket",
    "styled-components": "4.3.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "client": "yarn start",
    "server": "nodemon server.js",
    "server-build": "cd server && yarn build",
    "dev": "concurrently --kill-others-on-fail \"yarn client\" \"cd server && yarn start-dev\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "new-wallet": "cd server/build/utils/ && node userHelper.js --create wallet",
    "new-user": "f(){ cd server/build/utils/ && node userHelper.js --create user --role $1 --id $2 ;};f"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=8.0"
  },
  "proxy": "http://localhost:4000/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
