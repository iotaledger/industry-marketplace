{
  "name": "SeMarket",
  "version": "0.1.0",
  "description": "SeMarket",
  "repository": {
    "type": "git",
    "url": "https://github.com/iotaledger/SeMarket.git"
  },
  "private": true,
  "dependencies": {
    "@iota/area-codes": "^0.1.1",
    "@iota/converter": "^1.0.0-beta.19",
    "@iota/mam": "^0.7.3",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "express": "^4.17.1",
    "highlightjs": "^9.12.0",
    "lodash-es": "^4.17.15",
    "query-string": "^6.8.1",
    "react": "16.8.6",
    "react-cookie": "^4.0.1",
    "react-datepicker": "^2.8.0",
    "react-dom": "16.8.6",
    "react-md": "^1.12.2",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "socket.io-client": "^2.2.0",
    "styled-components": "4.3.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "install-all": "yarn && cd server && yarn",
    "client": "yarn start",
    "server": "nodemon server.js",
    "server-build": "cd server && yarn build",
    "dev": "yarn install-all && yarn server-build && concurrently --kill-others-on-fail \"yarn client\" \"cd server && yarn start-dev\"",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "new-wallet": "cd server/build/src/utils/ && node userHelper.js --create wallet",
    "new-user": "f(){ cd server/build/src/utils/ && node userHelper.js --create user --role $1 --name $2 --location $3;};f",
    "simulate": "f(){ cd server/build/src/utils/ && node simulationHelper.js --simulate --role $1 ;};f"
  
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "engines": {
    "node": ">=8.0"
  },
  "proxy": "http://localhost:5000/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
